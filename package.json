{
  "name":
    "tmcsplit",
  "version":
    "0.0.2",
  "description":
    "Basic Javascript implementation of GNU Split.",
  "funding": {
    "type":
      "patreon",
    "url":
      "https://patreon.com/tallero"
  },
  "man":
    "./man/tmcsplit.1",
  "files": [
    "AUTHORS.rst",
    "COPYING",
    "README.md",
    "eslint.config.mjs",
    "package.json",
    "tmcsplit.js",
    "tmcsplit",
    "fs-worker.js",
    "fs-worker.webpack.config.cjs",
    "index.html",
    "serve.json",
    "libtmcsplit.webpack.config.cjs",
    "webpack.config.cjs"
  ],
  "keywords": [
    "ur",
    "themartiancompany",
    "dogeos"
  ],
  "homepage":
    "https://github.com/themartiancompany/tmcsplit",
  "bugs": {
    "url":
      "https://github.com/themartiancompany/tmcsplit/issues"
  },
  "repository": {
    "type":
      "git",
    "url":
      "git+https://github.com/themartiancompany/tmcsplit.git"
  },
  "license":
    "AGPL-3.0-or-later",
  "author": {
    "name":
      "Pellegrino Prevete",
    "email":
      "pellegrinoprevete@gmail.com",
    "url":
      "https://github.com/tallero"
  },
  "devDependencies": {
    "crash-js-webpack":
      "^0.0.3",
    "eslint":
      "^10.0.0",
    "globals":
      "^17.3.0",
    "@eslint/js":
      "^10.0.1",
    "safely-serve":
      "^0.0.9",
    "webpack":
      "^5.102.1",
    "webpack-cli":
      "^6.0.1"
  },
  "dependencies": {
    "crash-js":
      "^0.1.73",
    "split-file":
      "npm:@themartiancompany/split-file@2.3.12"
  },
  "type":
    "commonjs",
  "main":
    "libtmcsplit",
  "bin":
    { "tmcsplit":
        "tmcsplit" },
  "scripts": {
    "build-fs-worker":
      "npx webpack --mode 'production' --config 'fs-worker.webpack.config.cjs' --stats-error-details",
    "build-lib":
      "npx webpack --mode 'production' --config 'libtmcsplit.webpack.config.cjs' --stats-error-details",
    "build-bin":
      "npx webpack --mode 'production' --config 'webpack.config.cjs' --stats-error-details",
    "build":
      "npm run lint && npm run build-fs-worker && npm run build-lib && npm run build-bin",
    "lint":
      "npx eslint .",
    "publish-pnm":
      "npm run build && npm publish --access public",
    "start":
      "npx safely-serve --help # index.html --port 3000 --https --no-cache",
    "test":
      "echo \"Error: no test specified\" && exit 1"
  }
}
